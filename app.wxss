@import 'style/weui.wxss';
@import "style/base/variable/color.wxss";
@import "style/base/variable/global.wxss";
@import "style/base/bodoni.wxss";
/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.numFontF{
  font-family: "bodoni";
}
swiper{height:100%;}
image{width:100%;height:100%;object-fit: cover;}
page{
    background-color: #f5f5f5;
    font-size: 16px;
    box-shadow: none;
    /*font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;*/
}
navigator{cursor: pointer;}
.page__hd {
    padding: 40px;
}
.page__bd {
    padding-bottom: 20px;
}
.page__bd_spacing {
    padding-left: 15px;
    padding-right: 15px;
}

.page__ft{
    padding-bottom: 10px;
    text-align: center;
}

.page__title {
    text-align: left;
    font-size: 20px;
    font-weight: 400;
}

.page__desc {
    margin-top: 5px;
    color: #888888;
    text-align: left;
    font-size: 14px;
}
.hoverSty{background: #e6e6e6;}

@keyframes loading {
  0% {transform: rotate(0deg)}
  50% {transform: rotate(180deg)}
  100% {transform: rotate(360deg)}
}

.loading {
  display: inline-block;
  transform-origin: 50% 50%;
  animation: loading 1s linear infinite;
}

@font-face {font-family: "iconfont";
  src: url('iconfont.eot?t=1520838078027'); /* IE9*/
  src: url('iconfont.eot?t=1520838078027#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
  url('iconfont.ttf?t=1520838078027') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
  url('iconfont.svg?t=1520838078027#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-loading:before { content: "\e9ac"; }

.fontsize20{font-size:20rpx;}
.fontsize24{font-size:24rpx;}
.fontsize28{font-size:28rpx;}
.fontsize30{font-size:30rpx;}
.fontsize36{font-size:36rpx;}
.fontsize40{font-size:40rpx;}
.fontsize48{font-size:48rpx;}
.fontweight_b{font-weight: bold;}
/*成长计划背景色---start*/
.bg_color_0{ background: linear-gradient(-5deg,#FF6130, #FB745F);}
.bg_color_1{ background: linear-gradient(-5deg,#5080FF,#3F67E9)}
.bg_color_2{ background: linear-gradient(-5deg,#3FCE7B,#2FC572);}
.bg_color_3{ background: linear-gradient(-5deg,#FEB721, #FECC21);}
.bg_color_4{ background: linear-gradient(-5deg,#CE7AFF,#A83DE8)}
